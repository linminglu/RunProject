- (UILabel *)makeLabelWithTitle:(NSString *)title content:(NSString *)content {
    UILabel *labelA = [[UILabel alloc] initWithFrame:CGRectZero];
    labelA.textColor = RGB(51, 51, 51);
    labelA.text = title;
    [self.contentView addSubview:labelA];
    labelA.sd_layout
    .topSpaceToView(self.contentView, 15)
    .leftSpaceToView(self.contentView, 15)
    .widthIs(80)
    .autoHeightRatio(0);
    
//    UILabel *labelB = [[UILabel alloc] initWithFrame:CGRectZero];
    
    return labelA;
}

